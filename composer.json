{
    "name": "iammordaty/musly-php",
    "description": "PHP wrapper around the `musly` command",
    "keywords": [ "musly", "music-similarity", "library", "wrapper", "php" ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "iammordaty",
            "email": "iammordaty@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/process": ">=4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Musly\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Musly\\Tests\\Unit\\": "tests/unit",
            "Musly\\Tests\\Integration\\": "tests/integration"
        }
    }
}
